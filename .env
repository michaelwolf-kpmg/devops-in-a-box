# This is the default domain name you want to subdomain each service off of 
# i.e. mydomain.com -> jenkins.mydomain.com
# DNS must route traffic for *.mydomain.com to the nginx server to handling proxy
DOS_HOST=localhost

# This is the default protocol that the application will be exposed on
DOS_PROTOCOL=http

# Used by system applications to authenticate, change in openldap-admin from default
DOS_LDAP_SYSTEM_PASSWORD=P@$$w0rd

# OpenLDAP https://github.com/osixia/docker-openldap#environment-variables
LDAP_ORGANISATION=DevOps
LDAP_DOMAIN=devops
LDAP_BASE_DN=dc=devops
LDAP_ADMIN_PASSWORD=P@$$w0rd
LDAP_CONFIG_PASSWORD=P@$$w0rd
LDAP_READONLY_USER=true
LDAP_READONLY_USER_USERNAME=readonly
LDAP_READONLY_USER_PASSWORD=P@$$w0rd
