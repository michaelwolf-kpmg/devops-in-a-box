# SonarQube https://hub.docker.com/_/sonarqube/
SONARQUBE_JDBC_PASSWORD=P@$$w0rd
SONARQUBE_JDBC_ROOT_PASSWORD=P@$$w0rd

# Collectd 
COLLECTD_PROXY_HOST=proxy
COLLECTD_OPENLDAP_HOST=openldap
COLLECTD_SONAR_DB_HOST=sonarqube_db
COLLECTD_OPENLDAP_ADMIN_HOST=openldap-admin
COLLECTD_NEXUS_HOST=nexus
COLLECTD_GITLAB_HOST=gitlab
COLLECTD_JENKINS_HOST=jenkins
COLLECTD_JENKINS_AGENT_HOST=jenkins-docker-agent
COLLECTD_SONARQUBE_HOST=sonarqube

#To enable LDAP Authentication for Grafana
GF_AUTH_LDAP_ENABLED=true
